resource "aws_db_instance" "example" {
  allocated_storage                     = 550
  allow_major_version_upgrade           = null
  apply_immediately                     = null
  auto_minor_version_upgrade            = true
  availability_zone                     = "eu-central-1b"
  backup_retention_period               = 7
  backup_target                         = "region"
  backup_window                         = "21:00-21:30"
  ca_cert_identifier                    = "rds-ca-2019"
  character_set_name                    = null
  copy_tags_to_snapshot                 = true
  custom_iam_instance_profile           = null
  customer_owned_ip_enabled             = false
  db_name                               = "<db-name>"
  db_subnet_group_name                  = "default"
  delete_automated_backups              = true
  deletion_protection                   = true
  domain                                = null
  domain_iam_role_name                  = null
  enabled_cloudwatch_logs_exports       = ["postgresql", "upgrade"]
  engine                                = "postgres"
  engine_version                        = "15.2"
  final_snapshot_identifier             = null
  iam_database_authentication_enabled   = false
  identifier                            = "prod-ke-encrypted"
  identifier_prefix                     = null
  instance_class                        = "db.m5.4xlarge"
  iops                                  = 12000
  kms_key_id                            = "<aws-kms-key-id>"
  license_model                         = "postgresql-license"
  maintenance_window                    = "fri:22:30-fri:23:30"
  manage_master_user_password           = null
  master_user_secret_kms_key_id         = null
  max_allocated_storage                 = 800
  monitoring_interval                   = 60
  monitoring_role_arn                   = "<aws-monitoring-role>"
  multi_az                              = true
  nchar_character_set_name              = null
  network_type                          = "IPV4"
  option_group_name                     = "default:postgres-15"
  parameter_group_name                  = "<parameter-group-name>"
  password                              = null # sensitive
  performance_insights_enabled          = true
  performance_insights_kms_key_id       = "<aws-performance-insightrs-key-id>"
  performance_insights_retention_period = 7
  port                                  = 51515
  publicly_accessible                   = true
  replica_mode                          = null
  replicate_source_db                   = null
  skip_final_snapshot                   = true
  snapshot_identifier                   = null
  storage_encrypted                     = true
  storage_throughput                    = 500
  storage_type                          = "gp3"
  tags = {
    Country             = "kenya"
    Environment         = "prod"
    Environment-Country = "prod-kenya"
    Service             = "<service-name>"
    Spend-Category      = "prod-kenya-<service-name>"
  }
  tags_all = {
    Country             = "kenya"
    Environment         = "prod"
    Environment-Country = "prod-kenya"
    Service             = "<service-name>"
    Spend-Category      = "prod-kenya-<service-name>"
  }
  timezone               = null
  username               = "<db-username>"
  vpc_security_group_ids = ["<sg-id>"]
}
